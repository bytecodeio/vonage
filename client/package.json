{
  "name": "extension-javascript-template",
  "version": "0.0.1",
  "description": "Bootstrapped Looker Extension with React and Javascript",
  "main": "dist/bundle.js",
  "scripts": {
    "analyze": "set ANALYZE_MODE=static && yarn build",
    "build": "set BABEL_ENV=build && webpack --config webpack.prod.js",
    "clean": "rm -rf dist",
    "dev": "webpack serve --hot --port 8080 --disable-host-check --config webpack.develop.js",
    "prebuild": "yarn clean"
  },
  "author": "yournamehere",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12"
  },
